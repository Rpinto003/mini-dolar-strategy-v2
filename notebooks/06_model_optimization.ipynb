{\n \"cells\": [\n  {\n   \"cell_type\": \"markdown\",\n   \"metadata\": {},\n   \"source\": [\n    \"# Otimização do Modelo WDO\\n\",\n    \"Otimização dos parâmetros e validação cruzada do modelo.\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"metadata\": {},\n   \"source\": [\n    \"import os\\n\",\n    \"import sys\\n\",\n    \"from pathlib import Path\\n\",\n    \"import pandas as pd\\n\",\n    \"import numpy as np\\n\",\n    \"from sklearn.model_selection import TimeSeriesSplit\\n\",\n    \"import matplotlib.pyplot as plt\\n\",\n    \"import seaborn as sns\\n\",\n    \"\\n\",\n    \"PROJECT_ROOT = Path(os.path.dirname(os.getcwd()))\\n\",\n    \"if str(PROJECT_ROOT) not in sys.path:\\n\",\n    \"    sys.path.append(str(PROJECT_ROOT))\\n\",\n    \"\\n\",\n    \"plt.style.use('seaborn')\\n\",\n    \"pd.set_option('display.max_columns', None)\"\n   ]\n  },\n  {\n   \"cell_type\": \"markdown\",\n   \"metadata\": {},\n   \"source\": [\n    \"## 1. Grid Search de Parâmetros\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"metadata\": {},\n   \"source\": [\n    \"# Carrega dados\\n\",\n    \"from src.data.loaders.market_data_loader import MarketDataLoader\\n\",\n    \"from src.ml.features.feature_engineering import FeatureEngineering\\n\",\n    \"from src.agents.market_agent import RiskManagementAgent, MarketRegimeAgent\\n\",\n    \"from src.agents.agent_coordinator import AgentCoordinator\\n\",\n    \"from src.backtest.monte_carlo import MonteCarloSimulator\\n\",\n    \"\\n\",\n    \"# Configuração dos parâmetros para teste\\n\",\n    \"param_grid = {\\n\",\n    \"    'risk_management': {\\n\",\n    \"        'max_drawdown': [-0.03, -0.04, -0.05, -0.06],\\n\",\n    \"        'max_position_size': [2, 3, 4, 5]\\n\",\n    \"    },\\n\",\n    \"    'market_regime': {\\n\",\n    \"        'window_size': [10, 15, 20, 25],\\n\",\n    \"        'volatility_threshold': [0.1, 0.15, 0.2]\\n\",\n    \"    }\\n\",\n    \"}\\n\",\n    \"\\n\",\n    \"# Carrega e prepara dados\\n\",\n    \"db_path = PROJECT_ROOT / 'src' / 'data' / 'database' / 'candles.db'\\n\",\n    \"loader = MarketDataLoader(str(db_path))\\n\",\n    \"data = loader.load_data()\\n\",\n    \"\\n\",\n    \"engineer = FeatureEngineering()\\n\",\n    \"features = engineer.create_technical_features(data)\\n\",\n    \"features = engineer.create_temporal_features(features)\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"metadata\": {},\n   \"source\": [\n    \"# Função para avaliar uma combinação de parâmetros\\n\",\n    \"def evaluate_parameters(features, risk_params, regime_params):\\n\",\n    \"    risk_agent = RiskManagementAgent(**risk_params)\\n\",\n    \"    regime_agent = MarketRegimeAgent(**regime_params)\\n\",\n    \"    coordinator = AgentCoordinator([risk_agent, regime_agent])\\n\",\n    \"    \\n\",\n    \"    # Validação cruzada com 5 folds\\n\",\n    \"    tscv = TimeSeriesSplit(n_splits=5)\\n\",\n    \"    metrics_list = []\\n\",\n    \"    \\n\",\n    \"    for train_idx, test_idx in tscv.split(features):\\n\",\n    \"        train_data = features.iloc[train_idx]\\n\",\n    \"        test_data = features.iloc[test_idx]\\n\",\n    \"        \\n\",\n    \"        # Simula com dados de teste\\n\",\n    \"        simulator = MonteCarloSimulator(n_simulations=100)\\n\",\n    \"        returns = test_data['returns'].dropna()\\n\",\n    \"        simulation = simulator.simulate_returns(\\n\",\n    \"            historical_returns=returns,\\n\",\n    \"            initial_capital=100000\\n\",\n    \"        )\\n\",\n    \"        \\n\",\n    \"        # Calcula métricas\\n\",\n    \"        metrics = {\\n\",\n    \"            'sharpe_ratio': simulation.sharpe_ratio,\\n\",\n    \"            'max_drawdown': simulation.max_drawdown,\\n\",\n    \"            'profit_factor': simulation.profit_factor,\\n\",\n    \"            'win_rate': simulation.win_rate\\n\",\n    \"        }\\n\",\n    \"        metrics_list.append(metrics)\\n\",\n    \"    \\n\",\n    \"    # Média das métricas entre os folds\\n\",\n    \"    avg_metrics = pd.DataFrame(metrics_list).mean()\\n\",\n    \"    return avg_metrics\\n\",\n    \"\\n\",\n    \"# Grid search\\n\",\n    \"results = []\\n\",\n    \"for max_dd in param_grid['risk_management']['max_drawdown']:\\n\",\n    \"    for pos_size in param_grid['risk_management']['max_position_size']:\\n\",\n    \"        for window in param_grid['market_regime']['window_size']:\\n\",\n    \"            for vol_thresh in param_grid['market_regime']['volatility_threshold']:\\n\",\n    \"                risk_params = {\\n\",\n    \"                    'max_drawdown': max_dd,\\n\",\n    \"                    'max_position_size': pos_size\\n\",\n    \"                }\\n\",\n    \"                regime_params = {\\n\",\n    \"                    'window_size': window,\\n\",\n    \"                    'volatility_threshold': vol_thresh\\n\",\n    \"                }\\n\",\n    \"                \\n\",\n    \"                metrics = evaluate_parameters(features, risk_params, regime_params)\\n\",\n    \"                results.append({\\n\",\n    \"                    **risk_params,\\n\",\n    \"                    **regime_params,\\n\",\n    \"                    **metrics.to_dict()\\n\",\n    \"                })\\n\",\n    \"\\n\",\n    \"results_df = pd.DataFrame(results)\\n\",\n    \"print('\\nMelhores parâmetros por métrica:')\\n\",\n    \"print('\\nMelhor Sharpe Ratio:')\\n\",\n    \"print(results_df.loc[results_df['sharpe_ratio'].idxmax()])\\n\",\n    \"\\n\",\n    \"print('\\nMenor Max Drawdown:')\\n\",\n    \"print(results_df.loc[results_df['max_drawdown'].idxmin()])\"\n   ]\n  },\n  {\n   \"cell_type\": \"markdown\",\n   \"metadata\": {},\n   \"source\": [\n    \"## 2. Validação dos Melhores Parâmetros\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"metadata\": {},\n   \"source\": [\n    \"# Usando os melhores parâmetros encontrados\\n\",\n    \"best_params_idx = results_df['sharpe_ratio'].idxmax()\\n\",\n    \"best_params = results_df.loc[best_params_idx]\\n\",\n    \"\\n\",\n    \"risk_agent = RiskManagementAgent(\\n\",\n    \"    max_drawdown=best_params['max_drawdown'],\\n\",\n    \"    max_position_size=best_params['max_position_size']\\n\",\n    \")\\n\",\n    \"\\n\",\n    \"regime_agent = MarketRegimeAgent(\\n\",\n    \"    window_size=best_params['window_size'],\\n\",\n    \"    volatility_threshold=best_params['volatility_threshold']\\n\",\n    \")\\n\",\n    \"\\n\",\n    \"coordinator = AgentCoordinator([risk_agent, regime_agent])\\n\",\n    \"\\n\",\n    \"# Teste final com dados mais recentes\\n\",\n    \"test_size = int(len(features) * 0.2)  # 20% mais recentes\\n\",\n    \"train_data = features[:-test_size]\\n\",\n    \"test_data = features[-test_size:]\\n\",\n    \"\\n\",\n    \"simulator = MonteCarloSimulator(n_simulations=1000)\\n\",\n    \"returns = test_data['returns'].dropna()\\n\",\n    \"simulation = simulator.simulate_returns(\\n\",\n    \"    historical_returns=returns,\\n\",\n    \"    initial_capital=100000\\n\",\n    \")\\n\",\n    \"\\n\",\n    \"print('\\nResultados Finais com Parâmetros Otimizados:')\\n\",\n    \"print(f'Sharpe Ratio: {simulation.sharpe_ratio:.2f}')\\n\",\n    \"print(f'Max Drawdown: {simulation.max_drawdown:.2%}')\\n\",\n    \"print(f'Win Rate: {simulation.win_rate:.2%}')\\n\",\n    \"print(f'Profit Factor: {simulation.profit_factor:.2f}')\"\n   ]\n  }\n ],\n \"metadata\": {\n  \"kernelspec\": {\n   \"display_name\": \"Python 3\",\n   \"language\": \"python\",\n   \"name\": \"python3\"\n  }\n },\n \"nbformat\": 4,\n \"nbformat_minor\": 4\n}